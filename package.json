{
	"name": "jup-grid",
	"version": "0.5.2",
	"description": "JupGrid",
	"type": "module",
	"main": "src/jupgrid.js",
	"author": {
		"name": "SpuddyA7X",
		"handle": "@SpuddyA7X"
	},
	"devDependencies": {
		"eslint": "^9.4.0"
	},
	"eslintConfig": {
		"parserOptions": {
			"ecmaVersion": "latest",
			"sourceType": "module"
		},
		"env": {
			"es6": true,
			"node": true,
			"commonjs": true
		},
		"extends": [
			"prettier",
			"standard"
		],
		"rules": {
			"no-console": "off",
			"no-tabs": "off",
			"indent": [
				"error",
				"tab"
			],
			"quotes": "off",
			"space-before-function-paren": "off",
			"semi": [
				"error",
				"always"
			]
		}
	},
	"scripts": {
		"fmt": "eslint . --fix",
		"lint": "eslint ."
	},
	"dependencies": {
		"@jup-ag/api": "^6.0.23",
		"@jup-ag/core": "4.0.0-beta.21",
		"@jup-ag/limit-order-sdk": "^0.1.10",
		"@project-serum/anchor": "^0.26.0",
		"@solana/spl-token": "^0.4.6",
		"@solana/web3.js": "^1.92.3",
		"axios": "^1.7.2",
		"bs58": "^5.0.0",
		"chalk": "^5.3.0",
		"cross-fetch": "^4.0.0",
		"crypto": "^1.0.1",
		"dotenv": "^16.4.5",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard": "^17.1.0",
		"express": "^4.19.2",
		"ora": "^8.0.1",
		"prompt-sync": "^4.2.0",
		"readline": "^1.3.0", 
		"winston": "^3.13.0",
		"ws": "^8.17.0"
	}
}
